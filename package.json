{
  "name": "@beequeue/dota-matches-api",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "CC-BY-SA-3.0-US",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "^24"
  },
  "simple-git-hooks": {
    "pre-commit": "node_modules/.bin/nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "wrangler dev -j",
    "dev:migrate": "wrangler d1 migrations apply --local dpc",
    "build": "tsdown",
    "lint": "eslint src",
    "test": "vitest",
    "test:e2e": "vitest  -c vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.2.1",
    "@cloudflare/vitest-pool-workers": "0.8.64",
    "@cloudflare/workers-types": "4.20250816.0",
    "@leeoniya/ufuzzy": "1.0.18",
    "@total-typescript/ts-reset": "0.6.1",
    "@tsconfig/node-ts": "23.6.1",
    "@tsconfig/node24": "24.0.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "24.3.0",
    "db0": "0.3.2",
    "discord-api-types": "0.38.20",
    "discord-interactions": "4.3.0",
    "es-toolkit": "1.39.10",
    "eslint": "9.33.0",
    "hono": "4.9.2",
    "kysely": "0.28.5",
    "mentoss": "0.11.0",
    "milli": "4.0.0",
    "miniflare": "4.20250813.1",
    "nano-staged": "0.8.0",
    "nanoid": "5.1.5",
    "p-queue": "8.1.0",
    "prettier": "3.6.2",
    "simple-git-hooks": "2.13.1",
    "temporal-polyfill": "0.3.0",
    "tsdown": "0.14.1",
    "typescript": "5.9.2",
    "ultrahtml": "1.6.0",
    "vitest": "3.2.4",
    "wrangler": "4.30.0",
    "xior": "0.7.8"
  }
}
