{
  "name": "@beequeue/dota-matches-api",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "CC-BY-SA-3.0-US",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^24"
  },
  "simple-git-hooks": {
    "pre-commit": "node_modules/.bin/nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "wrangler dev -j",
    "dev:migrate": "wrangler d1 migrations apply --local dpc",
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest --mode test",
    "test:e2e": "vitest --mode test --config vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.2",
    "@cloudflare/workers-types": "4.20241022.0",
    "@leeoniya/ufuzzy": "1.0.14",
    "@total-typescript/ts-reset": "0.6.1",
    "@tsconfig/node24": "24.0.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.15.24",
    "better-sqlite3": "11.5.0",
    "date-fns": "4.1.0",
    "discord-api-types": "0.37.103",
    "discord-interactions": "4.1.0",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.27.1",
    "drizzle-orm": "0.36.0",
    "eslint": "9.27.0",
    "hono": "4.6.8",
    "mande": "2.0.9",
    "milli": "4.0.0",
    "miniflare": "3.20241022.0",
    "nano-staged": "0.8.0",
    "nanoid": "5.0.9",
    "node-html-parser": "7.0.1",
    "p-queue": "8.0.1",
    "prettier": "3.5.3",
    "rambda": "9.3.0",
    "simple-git-hooks": "2.11.1",
    "tsup": "8.3.5",
    "typescript": "5.8.3",
    "undici-types": "7.10.0",
    "vitest": "3.0.2",
    "wrangler": "3.84.1"
  }
}
